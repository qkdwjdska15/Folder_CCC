<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TableView?>

<AnchorPane xmlns:fx="http://javafx.com/fxml/1"
	fx:controller="control.LessonTabController" prefHeight="500"
	prefWidth="700">

	<children>
		<Label text="과목 등록" layoutX="50.0" layoutY="30.0" />

		<Label text="과목 번호 :" layoutX="50.0" layoutY="72.0" />
		<TextField fx:id="txtLessonNum" layoutX="115.0"
			layoutY="70.0" promptText="2글자 이하" />

		<Label text="과목 명 : " layoutX="50.0" layoutY="102.0" />
		<TextField fx:id="txtLessonName" layoutX="115.0"
			layoutY="100.0" />

		<Button fx:id="btnLessonInsert" layoutX="50.0" layoutY="150.0"
			text="등록" />

		<Button fx:id="btnLessonUpdate" layoutX="100.0" layoutY="150.0"
			text="수정" />

		<Button fx:id="btnLessonDelete" layoutX="150.0" layoutY="150.0"
			text="삭제" />

		<Label text="과목 목록" layoutX="350.0" layoutY="30.0" />
		<TableView fx:id="lessonTableView" layoutX="350.0"
			layoutY="50.0" prefHeight="370" prefWidth="300">
		</TableView>
	</children>
</AnchorPane>

